 YMU business.","title":"The Brief","involvement":[{"item":"Web Development"},{"item":"Web Design"}],"featuredOn":null},"relatedProjects":{"projects":[{"__typename":"WordPress_Project","slug":"records-bars","categories":{"edges":[{"node":{"name":"Web Design"}},{"node":{"name":"Web Development"}}]},"workFields":{"featuredInfos":{"title":"Records Bars","image":{"sourceUrl":"https://content.kota.co.uk/app/uploads/2019/12/recordsbars-square.jpg","mediaItemId":2368,"modified":"2020-02-15 12:34:04","imageFile":{"childImageSharp":{"fluid":{"aspectRatio":1.1450381679389312,"src":"/static/690c99ea3c006da412d30aae0fa61463/8c3c2/recordsbars-square.jpg","srcSet":"/static/690c99ea3c006da412d30aae0fa61463/15aed/recordsbars-square.jpg 300w,\n/static/690c99ea3c006da412d30aae0fa61463/a07a5/recordsbars-square.jpg 600w,\n/static/690c99ea3c006da412d30aae0fa61463/8c3c2/recordsbars-square.jpg 1200w,\n/static/690c99ea3c006da412d30aae0fa61463/cf940/recordsbars-square.jpg 1500w","srcWebp":"/static/690c99ea3c006da412d30aae0fa61463/e7405/recordsbars-square.webp","srcSetWebp":"/static/690c99ea3c006da412d30aae0fa61463/4fec1/recordsbars-square.webp 300w,\n/static/690c99ea3c006da412d30aae0fa61463/483a3/recordsbars-square.webp 600w,\n/static/690c99ea3c006da412d30aae0fa61463/e7405/recordsbars-square.webp 1200w,\n/static/690c99ea3c006da412d30aae0fa61463/16643/recordsbars-square.webp 1500w","sizes":"(max-width: 1200px) 100vw, 1200px"}}}}}}},{"__typename":"WordPress_Project","slug":"linden-staub","categories":{"edges":[{"node":{"name":"Web Design"}},{"node":{"name":"Web Development"}}]},"workFields":{"featuredInfos":{"title":"Linden Staub","image":{"sourceUrl":"https://content.kota.co.uk/app/uploads/2019/10/linden-square.jpg","mediaItemId":2375,"modified":"2020-02-15 12:39:44","imageFile":{"childImageSharp":{"fluid":{"aspectRatio":1.1450381679389312,"src":"/static/86d5c884c379e705fcebb208226893dd/8c3c2/linden-square.jpg","srcSet":"/static/86d5c884c379e705fcebb208226893dd/15aed/linden-square.jpg 300w,\